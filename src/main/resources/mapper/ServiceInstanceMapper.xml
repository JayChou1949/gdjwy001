<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.upd.hwcloud.dao.ServiceInstanceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.upd.hwcloud.bean.entity.ServiceInstance">
        <id column="ID" property="id" />
        <result column="NAME" property="name" />
        <result column="ALIAS" property="alias" />
        <result column="SERVICE_NAME" property="serviceName" />
        <result column="SERVICE_ALIAS" property="serviceAlias" />
        <result column="SERVICE_GUID" property="serviceGuid" />
        <result column="SERVICE_PLAN_NAME" property="servicePlanName" />
        <result column="INSTANCE_TYPE" property="instanceType" />
        <result column="ACTUAL_ID" property="actualId" />
        <result column="STATUS" property="status" />
        <result column="CREATED_AT" property="createdAt" />
        <result column="UPDATED_AT" property="updatedAt" />
        <result column="ACTUAL_NAME" property="actualName" />
        <result column="GUID" property="guid" />
        <result column="DOMAIN_ID" property="domainId" />
        <result column="PROJECT_ID" property="projectId" />
        <result column="AREA_NAME" property="areaName" />
        <result column="POLICE_CATEGORY" property="policeCategory" />
        <result column="USER_ID" property="userId" />
        <result column="CATA_LOG" property="cataLog" />
        <result column="APP_NAME" property="appName" />
    </resultMap>

    <select id="getServiceInstanceByProid" resultMap="BaseResultMap">
        SELECT * FROM TB_SERVICE_INSTANCE tsi WHERE tsi.PROJECT_ID = #{projectId}
    </select>
</mapper>
